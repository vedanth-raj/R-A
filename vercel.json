{
  "version": 2,
  "builds": [
    {
      "src": "web_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_app.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "SEMANTIC_SCHOLAR_API_KEY": "@semantic_scholar_api_key"
  }
}
